  </model>	

  <!-- Hook -->
  <animation>
    <type>rotate</type>
    <object-name>Hook.001</object-name>
    <property>gear/tailhook/position-norm</property>
    <factor>60</factor>
    <center>
      <x-m>12.18</x-m> <!-- <x-m>12.5454</x-m> -->
      <y-m>0</y-m>
      <z-m>-0.5</z-m>
    </center>
    <axis><y>10</y></axis>
  </animation>

  <!-- Canopy -->
  <animation>
    <type>rotate</type>
    <object-name>canopyglass</object-name>
    <object-name>canopyframe</object-name>
    <property>/controls/canopy/position-norm</property>
    <factor>35</factor>
    <center>
      <x-m>6.04</x-m>
      <y-m>0.0</y-m>
      <z-m>1.294</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <!-- Lights 
  <model>
    <name>Navigation Lights</name>
    <path>Aircraft/EF-Typhoon/Models/nav-lights.xml</path>
  </model> 
  sim/multiplay/generic/float[4]="systems/electrical/outputs/beacon/state"
  sim/multiplay/generic/float[5]="systems/electrical/outputs/strobe/state"
  sim/multiplay/generic/float[6]="systems/electrical/outputs/nav-lights/state"
  sim/multiplay/generic/float[7]="systems/electrical/outputs/landing-lights/state"
  -->

  <!-- Landing Lights -->
  <animation>
    <type>noshadow</type>
    <object-name>gearlight.R</object-name>
    <object-name>gearlight.L</object-name>
  </animation>

  <animation>
    <type>alpha-test</type>
    <object-name>gearlight.R</object-name>
    <object-name>gearlight.L</object-name>
    <alpha-factor>0.01</alpha-factor>
  </animation>

  <animation>
    <type>select</type>
    <object-name>gearlight.L</object-name>
    <condition>
      <!--
      <greater-than>
	<property>sim/time/sun-angle-rad</property>
	<value>1.32</value>
      </greater-than>
      -->
      <and>
	<greater-than>
          <property>/sim/multiplay/generic/float[7]</property>
          <value>0.2</value>
	</greater-than>
	<equals>
          <property>gear/gear/position-norm</property>
          <value>1</value>
	</equals>
      </and>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>gearlight.R</object-name>
    <condition>
      <!--
      <greater-than>
	<property>sim/time/sun-angle-rad</property>
	<value>1.32</value>
      </greater-than>
      -->
      <and>
	<greater-than>
          <property>/sim/multiplay/generic/float[7]</property>
          <value>0.2</value>
	</greater-than>
	<equals>
          <property>gear/gear/position-norm</property>
          <value>1</value>
	</equals>
      </and>
    </condition>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>Light.L</object-name>
    <object-name>Light.R</object-name>
    <object-name>Light.rear</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>Light.beacup</object-name>
    <object-name>Light.beacon.d.L</object-name>
    <object-name>Light.beacon.d.R</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>Light.strobeup</object-name>
    <object-name>Light.strobe.d.L</object-name>
    <object-name>Light.strobe.d.R</object-name>
  </animation>

  <animation>
    <type>alpha-test</type>
    <object-name>Light.L</object-name>
    <object-name>Light.R</object-name>
    <object-name>Light.rear</object-name>
    <alpha-factor>0.01</alpha-factor>
  </animation>

  <animation>
    <type>alpha-test</type>
    <object-name>Light.beacup</object-name>
    <object-name>Light.beacon.d.L</object-name>
    <object-name>Light.beacon.d.R</object-name>
    <alpha-factor>0.01</alpha-factor>
  </animation>

  <animation>
    <type>alpha-test</type>
    <object-name>Light.strobeup</object-name>
    <object-name>Light.strobe.d.L</object-name>
    <object-name>Light.strobe.d.R</object-name>
    <alpha-factor>0.01</alpha-factor>
  </animation>

  <!-- Flash Lights -->
  <animation>
    <type>select</type>
    <object-name>Light.beacon.d.R</object-name>
    <condition>
      <and>
	<equals>
          <property>sim/model/livery/RAFlights</property>
	  <value>1</value>
	</equals>
	<and>
	  <property>sim/model/livery/beacon</property>
	  <equals>
            <property>/sim/multiplay/generic/float[4]</property>
            <value>1</value>
	  </equals>
	</and>
      </and>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>Light.strobe.d.R</object-name>
    <condition>
      <and>
	<equals>
          <property>sim/model/livery/RAFlights</property>
	  <value>1</value>
	</equals>
	<and>
	  <property>sim/model/livery/strobe</property>
	  <equals>
            <property>/sim/multiplay/generic/float[5]</property>
            <value>1</value>
	  </equals>
	</and>
      </and>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>Light.beacon.d.L</object-name>
    <condition>
      <and>
	<equals>
          <property>sim/model/livery/RAFlights</property>
	  <value>0</value>
	</equals>
	<and>
	  <property>sim/model/livery/beacon</property>
	  <equals>
            <property>/sim/multiplay/generic/float[4]</property>
            <value>1</value>
	  </equals>
	</and>
      </and>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>Light.strobe.d.L</object-name>
    <condition>
      <and>
	<equals>
          <property>sim/model/livery/RAFlights</property>
	  <value>0</value>
	</equals>
	<and>
	  <property>sim/model/livery/strobe</property>
	  <equals>
            <property>/sim/multiplay/generic/float[5]</property>
            <value>1</value>
	  </equals>
	</and>
      </and>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>beacon.d.L</object-name>
    <condition>
      <and>
	<equals>
	  <property>sim/model/livery/beacon</property>
          <value>1</value>
	</equals>
	<equals>
	  <property>sim/model/livery/RAFlights</property>
          <value>0</value>
	</equals>
      </and>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>strobe.d.L</object-name>
    <condition>
      <and>
	<equals>
	  <property>sim/model/livery/strobe</property>
          <value>1</value>
	</equals>
	<equals>
	  <property>sim/model/livery/RAFlights</property>
          <value>0</value>
	</equals>
      </and>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>beacon.d.R</object-name>
    <condition>
      <and>
	<equals>
	  <property>sim/model/livery/beacon</property>
          <value>1</value>
	</equals>
	<equals>
	  <property>sim/model/livery/RAFlights</property>
          <value>1</value>
	</equals>
      </and>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>strobe.d.R</object-name>
    <condition>
      <and>
	<equals>
	  <property>sim/model/livery/strobe</property>
          <value>1</value>
	</equals>
	<equals>
	  <property>sim/model/livery/RAFlights</property>
          <value>1</value>
	</equals>
      </and>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>Light.beacup</object-name>
    <condition>
      <property>/sim/multiplay/generic/float[4]</property>
      <property>sim/model/livery/beacon</property>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>Light.strobeup</object-name>
    <condition>
      <property>/sim/multiplay/generic/float[5]</property>
      <property>sim/model/livery/strobe</property>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>strobe.up</object-name>
    <condition>
      <property>sim/model/livery/strobe</property>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>beacon.up</object-name>
    <condition>
      <property>sim/model/livery/beacon</property>
    </condition>
  </animation>

  <!-- Nav Lights-->
  <animation>
    <type>select</type>
    <object-name>Light.L</object-name>
    <object-name>Light.R</object-name>
    <object-name>Light.rear</object-name>
    <condition>
      <and>
	<equals>
          <property>/sim/multiplay/generic/float[6]</property>
	  <value>1</value>
	</equals>
	<greater-than>
	  <property>sim/time/sun-angle-rad</property>
	  <value>1.32</value>
	</greater-than>
      </and>
    </condition>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>Light.L</object-name>
    <object-name>Light.R</object-name>
    <object-name>Light.rear</object-name>
  </animation>

  <animation>
    <type>alpha-test</type>
    <object-name>Light.L</object-name>
    <object-name>Light.R</object-name>
    <object-name>Light.rear</object-name>
    <alpha-factor>0.01</alpha-factor>
  </animation>

  <animation>
    <type>material</type> 
    <object-name>lens.R</object-name>
    <condition>
      <equals>
	<property>/sim/multiplay/generic/float[6]</property>
	<value>1</value>
      </equals>
    </condition>
    <emission>	
      <red>0</red>	
      <green>1</green>	
      <blue>0</blue>
    </emission>
  </animation>

  <animation>
    <type>material</type> 
    <object-name>lens.L</object-name>
    <condition>
      <equals>
	<property>/sim/multiplay/generic/float[6]</property>
	<value>1</value>
      </equals>
    </condition>
    <emission>	
      <red>1</red>	
      <green>0</green>	
      <blue>0</blue>
    </emission>
  </animation>

  <animation>
    <type>material</type> 
    <object-name>lens.rear</object-name>
    <condition>
      <equals>
	<property>/sim/multiplay/generic/float[6]</property>
	<value>1</value>
      </equals>
    </condition>
    <emission>	
      <red>1</red>	
      <green>1</green>	
      <blue>1</blue>
    </emission>
  </animation>

  <!-- layout selection f16 model dragchute animation
  <animation>
    <type>select</type>
    <object-name>USAF_Tailroot</object-name>
    <condition>
      <not>
	<property>sim/model/f16/dragchute</property>
      </not>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>RNLAF_Tailroot</object-name>
    <condition>
      <property>sim/model/f16/dragchute</property>
    </condition>
  </animation>
  -->
  
  <!-- Gears light cone original form F106 disabled. Solved with a half cone! -->
  <model>
    <name>ConeLeft</name>
    <path>Aircraft/EF-Typhoon/Models/light-cone.xml</path>
    <offsets>
      <x-m>9.386</x-m>
      <y-m>-2.057</y-m>
      <z-m>-0.829</z-m>
      <heading-deg>  7.500 </heading-deg>
      <pitch-deg>   -7.000 </pitch-deg>
    </offsets>
  </model>

  <model>
    <name>ConeRight</name>
    <path>Aircraft/EF-Typhoon/Models/light-cone.xml</path>
    <offsets>
      <x-m>9.386</x-m>
      <y-m>2.057</y-m>
      <z-m>-0.829</z-m>
      <heading-deg> -7.500 </heading-deg>
      <pitch-deg>   -7.000 </pitch-deg>
    </offsets>
  </model>

  <animation>
    <type>select</type>
    <object-name>ConeLeft</object-name>
    <object-name>ConeRight</object-name>
    <condition>
      <and>
	<greater-than>
          <property>/sim/multiplay/generic/float[7]</property>
          <value>0.2</value>
	</greater-than>
	<and>
	  <equals>
	    <property>gear/gear[0]/position-norm</property>
	    <value>1</value>
	  </equals>
	  <greater-than>
	    <property>sim/time/sun-angle-rad</property>
	    <value>1.32</value>
	  </greater-than>
	</and>
      </and>
    </condition>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>ConeLeft</object-name>
    <object-name>ConeRight</object-name>
  </animation>

  <animation>
    <type>alpha-test</type>
    <object-name>ConeLeft</object-name>
    <object-name>ConeRight</object-name>
    <alpha-factor>0.01</alpha-factor>
  </animation>

</PropertyList>
